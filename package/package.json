{
  "name": "@bccampus/mantinex-composite",
  "version": "0.1.0",
  "description": "Mantine Composite component",
  "homepage": "https://github.com/bccampus/mantinex-composite",
  "packageManager": "yarn@4.0.1",
  "license": "MIT",
  "author": "Kemal Tunador <ktunador@bccampus.ca>",
  "keywords": [
    "extension",
    "mantine"
  ],
  "main": "./dist/cjs/index.cjs",
  "module": "./dist/esm/index.mjs",
  "types": "./dist/types/index.d.ts",
  "exports": {
    ".": {
      "import": {
        "types": "./dist/types/index.d.mts",
        "default": "./dist/esm/index.mjs"
      },
      "require": {
        "types": "./dist/types/index.d.ts",
        "default": "./dist/cjs/index.cjs"
      }
    },
    "./styles.css": "./dist/styles.css",
    "./styles.layer.css": "./dist/styles.layer.css"
  },
  "repository": "https://github.com/bccampus/mantinex-composite.git",
  "workspaces": [
    "docs"
  ],
  "peerDependencies": {
    "@mantine/core": ">=7.0.0",
    "@mantine/hooks": ">=7.0.0",
    "react": "^18.2.0",
    "react-dom": "^18.2.0"
  },
  "bugs": "https://github.com/bccampus/mantinex-composite/issues"
}
